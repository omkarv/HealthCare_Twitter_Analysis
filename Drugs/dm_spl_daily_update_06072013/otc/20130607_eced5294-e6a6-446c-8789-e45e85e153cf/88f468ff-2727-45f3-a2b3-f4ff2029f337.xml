               <id root="a85c9861-0270-4283-a4f2-adc1e2200a1b"/>
               <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
               <title>Purpose</title>
               <text>
                  <paragraph>Antihistamine</paragraph>
