         <component>
            <section ID="i4i_OTC_Purpose_id_4554f2e1-e8fc-44b8-b925-270bd5cbc0ac">
               <id root="6b804388-c72b-4013-81a2-08938ed5630a"/>
               <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
               <title>Purpose</title>
               <text>
                  <paragraph>Antihistamine</paragraph>
               </text>
