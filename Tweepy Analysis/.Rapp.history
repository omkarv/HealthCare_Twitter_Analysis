gettingtext = function(mydata){#
#
	mydata <- as.matrix(mydata)#
	new <- c()#
#
	if(dim(mydata)[2]>1){#
 		for(j in 1:dim(mydata)[1]){#
			temp <- c()#
			for(k in 1:dim(mydata)[2]){#
				temp <- paste(temp, mydata[j,k])#
			}#
			new <- rbind(new,trim(temp))#
		}#
		mydata <- new#
	}#
	data <- c()#
	for(i in 1:length(mydata)){#
		if(mydata[i]=="lang en"){#
			m <- mydata[i-1]#
			while(m==""){#
				num =  num-1#
				m <- mydata[num-1]#
			}#
			n <- m#
			m.split <- unlist(strsplit(m," "))#
			num <- i-1#
			while(!(m.split[1]=="Tweet" & m.split[2]=="text:")){#
				m <- mydata[num-1]#
				while(m==""){	#
					num =  num-1#
					m <- mydata[num-1]#
				}#
				m.split <- unlist(strsplit(m," "))	#
				n <- paste(m,n)#
				num = num - 1#
			}#
			if(nchar(n)<500){#
			data <- rbind(data,n)#
			}#
		}#
	}#
	setwd(path2)#
	write.table(data, file = "outputmw.csv", row.names=F,sep="^")#
	setwd(path)#
	return(data)#
}#
